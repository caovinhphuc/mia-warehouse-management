# ==================== MIA WAREHOUSE MANAGEMENT SYSTEM ====================
# Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# ==================== GOOGLE SHEETS INTEGRATION ====================
# Google Sheets API Key - Get from Google Cloud Console
REACT_APP_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here

# Google Sheets ID - Get from Google Sheets URL
REACT_APP_GOOGLE_SHEETS_ID=your_google_sheets_id_here

# ==================== GOOGLE APPS SCRIPT WEBHOOKS ====================
# Google Apps Script Web App URLs for real-time operations
REACT_APP_AUDIT_WEBHOOK_URL=your_google_apps_script_web_app_url_here
REACT_APP_PROFILE_UPDATE_WEBHOOK_URL=your_google_apps_script_web_app_url_here

# ==================== PRODUCTION SETTINGS ====================
# Application Environment
NODE_ENV=production

# Application URL (for Vercel deployment)
REACT_APP_BASE_URL=https://your-vercel-app-url.vercel.app

# ==================== OPTIONAL FEATURES ====================
# Enable debug mode
REACT_APP_DEBUG=false

# API timeout settings (milliseconds)
REACT_APP_API_TIMEOUT=10000

# Cache TTL (milliseconds)
REACT_APP_CACHE_TTL=300000

